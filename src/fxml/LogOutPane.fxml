<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="800.0" stylesheets="@../css/log-out-pane.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.LogOutPaneController">
   <children>
      <StackPane layoutX="10.0" layoutY="50.0" AnchorPane.bottomAnchor="60.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="78.0">
         <children>
            <VBox alignment="TOP_CENTER" maxWidth="600.0" minHeight="200.0" minWidth="600.0" prefHeight="360.0" prefWidth="600.0" styleClass="vbox">
               <children>
                  <Label maxWidth="600.0" minWidth="600.0" prefHeight="26.0" prefWidth="600.0" styleClass="header" text="Log out">
                     <padding>
                        <Insets left="30.0" />
                     </padding>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Label>
                  <Label text="Do you want to log out?">
                     <VBox.margin>
                        <Insets top="200.0" />
                     </VBox.margin>
                  </Label>
                  <Button minHeight="35.0" mnemonicParsing="false" onAction="#handleYesBtn" prefHeight="35.0" prefWidth="190.0" text="Yes">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                     <styleClass>
                        <String fx:value="btn" />
                        <String fx:value="yes" />
                     </styleClass>
                  </Button>
               </children>
            </VBox>
         </children>
      </StackPane>
   </children>
</AnchorPane>
